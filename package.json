{
  "name": "boiler-plate",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "test": "jest --collectCoverage",
    "prebuild": "rimraf dist/",
    "build": "tsc",
    "start": "ts-node src/index.ts",
    "serve": "node dist/index.js",
    "dev": "nodemon",
    "watch-ts": "tsc -w",
    "migration:generate": "ts-node ./node_modules/typeorm/cli.js  migration:generate -d ./src/configs/ormconfig.ts ./src/migrations/",
    "migration:run": "ts-node ./node_modules/typeorm/cli.js  migration:run -d ./src/configs/ormconfig.ts",
    "migration:revert": "ts-node ./node_modules/typeorm/cli.js migration:revert"
  },
